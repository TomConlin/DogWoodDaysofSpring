select site_id, first_yes_year as year, phenophase_description,
	count(distinct individual_id) individual_clones,

	round(min(first_yes_doy)) early_doy,
    round(max(first_yes_doy)) late_doy,
    round(max(first_yes_doy)-min(first_yes_doy)) diff_doy,
    round(avg(first_yes_doy)) avg_doy, 
	round(stddev(first_yes_doy)) stddev_doy, 

	round(min(last_yes_doy)) early_last_doy,
    round(max(last_yes_doy)) late_last_doy,
    round(max(last_yes_doy)-min(last_yes_doy)) diff_last_doy,
    round(avg(last_yes_doy))  avg_last_doy,
    round(stddev(last_yes_doy)) stddev_last_doy

