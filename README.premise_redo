subset clones "first reports"

```
select 
	site_id  ,
	latitude ,
	longitude,
	elevation_in_meters  ,
	state  ,
	species_id  ,
	species  ,
	common_name  ,
	individual_id  ,
	phenophase_id  ,
	phenophase_description  ,
	first_yes_year  ,
	min(first_yes_doy) first_yes_doy ,
	min(first_yes_julian_date)  first_yes_julian_date,
	min(last_yes_doy ) last_yes_doy,
	min(last_yes_julian_date) last_yes_julian_date,
	location   -- postgis representation of lon/lat
into table clone_first_report_doy
from dogwood_spp
where common_name = 'Appalachian Spring dogwood'
group by
	site_id  ,
	latitude ,
	longitude,
	elevation_in_meters  ,
	state  ,
	species_id  ,
	species  ,
	common_name  ,
	individual_id  ,
	phenophase_id  ,
	phenophase_description  ,
	first_yes_year,
    location 
;
SELECT 2402

```

clone_first_report_doy
that is all the all the multiple observations of individual clones for all phenotype s
reduced to first come first serve, winner takes all.


----------------------------------

select zone zonetype, first_yes_year as year,  phenophase_id, phenophase_description,
	count(distinct individual_id) individual_clones,

	round(min(first_yes_doy)) early_doy,
    round(max(first_yes_doy)) late_doy,
    round(max(first_yes_doy)-min(first_yes_doy)) diff_doy,
    round(avg(first_yes_doy)) avg_doy, 
	round(stddev(first_yes_doy)) stddev_doy, 

	round(min(last_yes_doy)) early_last_doy,
    round(max(last_yes_doy)) late_last_doy,
    round(max(last_yes_doy)-min(last_yes_doy)) diff_last_doy,
    round(avg(last_yes_doy))  avg_last_doy,
    round(stddev(last_yes_doy)) stddev_last_doy

into clones_zones_firstdoy_stats
 from int_loc_zone ilz  -- pre-generated intersection table
 join  clone_first_report_doy dw on ilz.site_id = dw.site_id
 join ohm_us_shp pus on ilz.gid = pus.gid
where phenophase_id  = 483
group by 1,2,3,4

union
select zone zonetype, first_yes_year as year,  phenophase_id, phenophase_description,
	count(distinct individual_id) individual_clones,

	round(min(first_yes_doy)) early_doy,
    round(max(first_yes_doy)) late_doy,
    round(max(first_yes_doy)-min(first_yes_doy)) diff_doy,
    round(avg(first_yes_doy)) avg_doy, 
	round(stddev(first_yes_doy)) stddev_doy, 

	round(min(last_yes_doy)) early_last_doy,
    round(max(last_yes_doy)) late_last_doy,
    round(max(last_yes_doy)-min(last_yes_doy)) diff_last_doy,
    round(avg(last_yes_doy))  avg_last_doy,
    round(stddev(last_yes_doy)) stddev_last_doy

 from int_loc_zone ilz  -- pre-generated intersection table
 join  clone_first_report_doy dw on ilz.site_id = dw.site_id
 join ohm_us_shp pus on ilz.gid = pus.gid
where phenophase_id  = 500
group by 1,2,3,4

union
select zone zonetype, first_yes_year as year,  phenophase_id, phenophase_description,
	count(distinct individual_id) individual_clones,

	round(min(first_yes_doy)) early_doy,
    round(max(first_yes_doy)) late_doy,
    round(max(first_yes_doy)-min(first_yes_doy)) diff_doy,
    round(avg(first_yes_doy)) avg_doy, 
	round(stddev(first_yes_doy)) stddev_doy, 

	round(min(last_yes_doy)) early_last_doy,
    round(max(last_yes_doy)) late_last_doy,
    round(max(last_yes_doy)-min(last_yes_doy)) diff_last_doy,
    round(avg(last_yes_doy))  avg_last_doy,
    round(stddev(last_yes_doy)) stddev_last_doy

 from int_loc_zone ilz  -- pre-generated intersection table
 join  clone_first_report_doy dw on ilz.site_id = dw.site_id
 join ohm_us_shp pus on ilz.gid = pus.gid
where phenophase_id  = 498
group by 1,2,3,4
;

