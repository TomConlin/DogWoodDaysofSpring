91 events for the phenophases we are concerning ourselves with
which have  with 2 - 3 records.




copy (

with 
earlybird as (
 
select individual_id, latitude,longitude, phenophase_id,first_yes_year, min(first_yes_doy) first_yes_doy
  from dogwood_spp 
  where common_name = 'Appalachian Spring dogwood'
	and phenophase_id  in (483, 500, 498)
   group by 1,2,3,4,5
),
outlier as (

select latitude,longitude,phenophase_id, first_yes_year
  from earlybird 
   group by 1,2,3,4
   having count(distinct individual_id) > 1	
	and ((max(first_yes_doy) - min(first_yes_doy)) / 7) > 4  -- ~ a month
)
select dw.first_yes_year, min(first_yes_doy),  phenophase_description, dw.site_id, dw.individual_id
  from outlier oldw 
  join dogwood_spp dw 
    on dw.latitude = oldw.latitude 
   and dw.longitude = oldw.longitude 
   and dw.phenophase_id = oldw.phenophase_id
   and dw.first_yes_year = oldw.first_yes_year
  where common_name = 'Appalachian Spring dogwood'
group by 1,3,4,5 order by 1,2,3

) to STDOUT
;

